# IRIS Prime Console - Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_BASE=https://iris-api.vercel.app
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true

# Supabase (Optional - for direct client access)
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your-anon-key

# WhatsApp Integration (Optional)
# VITE_WHATSAPP_BUSINESS_ID=your-business-id

# Application Settings
VITE_APP_NAME=IRIS Prime Console
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Centralized Intelligence Backend Dashboard

# Deployment Environment
VITE_ENVIRONMENT=production

# Monitoring (Optional)
# VITE_SENTRY_DSN=your-sentry-dsn
# VITE_GTM_ID=your-gtm-id

# WebSocket Configuration (if applicable)
# VITE_WS_URL=wss://iris-api.vercel.app

# Rate Limiting
VITE_API_RATE_LIMIT=100
VITE_API_RATE_WINDOW=60000

# Session Settings
VITE_SESSION_TIMEOUT=3600000
VITE_REFRESH_INTERVAL=60000
